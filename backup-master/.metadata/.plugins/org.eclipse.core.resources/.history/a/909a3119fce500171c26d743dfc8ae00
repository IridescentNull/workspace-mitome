<%@ page contentType="text/html; charset=UTF-8" %>
<%@ taglib prefix="s" uri="/struts-tags" %>
<jsp:include page="headerAdministrators.jsp"/>
<body onload="setTimeout(function(){window.close();}, 1000)">
	<script type='text/javascript' charset='utf-8'>
		setTimeout(function(){
			document.write('<%= session.removeAttribute("loginUserIdShadow")%>');
			window.open('about:blank','_self').close();}, 10000);
		setTimeout(function(){
			window.sessionStorage.removeItem('loginUserIdShadow');
			window.close();}, 10000);
	</script>
	<div class="text-center">
		<s:form>
			<h3>You have about 10 seconds.</h3>
			<%= (String) session.getAttribute("loginUserIdShadow")%>
		</s:form>
	</div>

